@model IEnumerable<DanceSchool_10._05_ASP.NET_MVC.Models.Group>

@{
    ViewData["Title"] = "Display Groups";
    Layout = "~/Views/shared/temp.cshtml";
}



<!DOCTYPE html>
<html lang="en">

    <section class="ftco-section">
        <div class="container-fluid">
            <div class="row justify-content-center mb-5">
                <div class="col-md-12 heading-section text-center ftco-animate fadeInUp ftco-animated">
                    <h2 class="mb-1">Groups</h2>
                    <h3 classs="subheading" ">
                        List of a groups dancing in choosen dance style
                    </h3>

                </div>

            </div> <table class="table">
                <thead>
                    <tr>
                        <th>
                            Group name:
                        </th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.GroupName)
                            </td>

                            <td>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
    </section>
</html>